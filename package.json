{
  "name": "@kikiutils/stylelint-config",
  "type": "module",
  "version": "0.3.0",
  "description": "",
  "author": "kiki-kanri",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kikiutils/stylelint-config.git"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": null
    },
    "./*": "./dist/*.js",
    "./index": {
      "types": null,
      "import": null,
      "require": null
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist",
    "./src"
  ],
  "engines": {
    "node": ">=22.12.0"
  },
  "scripts": {
    "build": "tsdown",
    "bumplog": "changelogen --bump --hideAuthorEmail",
    "depcheck": "depcheck",
    "lint": "eslint --max-warnings 0",
    "lint:fix": "eslint --fix --max-warnings 0",
    "prepack": "pnpm run build",
    "release": "pnpm run lint && pnpm run typecheck && pnpm run test && pnpm run build && changelogen --hideAuthorEmail --push --release && npm publish",
    "test": "vitest run --coverage",
    "test:watch": "vitest watch --coverage",
    "typecheck": "tsc -b --noEmit",
    "unused-exports": "ts-unused-exports ./tsconfig.json"
  },
  "peerDependenciesMeta": {},
  "dependencies": {
    "lodash": "^4.17.21",
    "stylelint": "^16.26.0",
    "stylelint-config-clean-order": "^8.0.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@kikiutils/eslint-config": "^5.0.0",
    "@kikiutils/tsconfigs": "^5.0.5",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.14",
    "changelogen": "^0.6.2",
    "depcheck": "^1.4.7",
    "publint": "^0.3.15",
    "ts-unused-exports": "^11.0.1",
    "tsdown": "^0.16.8",
    "typescript": "^5.9.3",
    "unplugin-unused": "^0.5.6",
    "vitest": "^4.0.14"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
